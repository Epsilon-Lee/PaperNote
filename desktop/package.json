{
    "name": "PaperNote",
    "version": "1.0.0",
    "description": "A cross-platform application for reading and noting papers.",
    "main": "main.ts",
    "scripts": {
        "build": "tsc",
        "start": "tsc && electron main.js",
        "package": "npm run build && electron-packager . --out=dist --platform=all --arch=all --overwrite"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hzxie/PaperNote.git"
    },
    "keywords": [
        "PaperNote",
        "Electron"
    ],
    "author": "hzxie",
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/hzxie/PaperNote/issues"
    },
    "homepage": "https://papernote.org",
    "dependencies": {
        "@angular/common": "~2.3.0",
        "@angular/compiler": "~2.3.0",
        "@angular/core": "~2.3.0",
        "@angular/http": "~2.3.0",
        "@angular/material": "2.0.0-alpha.11-3",
        "@angular/platform-browser": "~2.3.0",
        "@angular/platform-browser-dynamic": "~2.3.0",
        "electron-prebuilt": "^1.4.12",
        "electron-packager": "^8.4.0",
        "typescript": "~2.1.4",
        "webpack": "^1.14.0",
        "rxjs": "5.0.0-rc.4",
        "zone.js": "^0.7.2"
    }
}
